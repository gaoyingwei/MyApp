<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable  name="inputview">
        <!--图片-->
        <attr name="input_icon" format="reference"></attr>
        <!--字体-->
        <attr name="input_hint" format="string"></attr>
        <!--是否密文-->
        <attr name="is_pass" format="boolean"></attr>
    </declare-styleable>


    <declare-styleable  name="nav">
        <!--图片-->
        <attr name="icon" format="reference"></attr>
        <!--字体-->
        <attr name="hint" format="string"></attr>
        <!--是否密文-->
        <attr name="pass" format="boolean"></attr>
        <attr name="right_icon" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="IndexBar">
        <attr name="letterSize" format="dimension" />
        <attr name="letterColor" format="color" />
        <attr name="focusLetterColor" format="color" />
        <attr name="letterSpacingExtra" format="float" />
    </declare-styleable>

    <declare-styleable name="HeartViewGroup">
        <attr name="heart_swipe_image" format="integer" />
        <attr name="heart_shake" format="boolean" />
        <attr name="heart_refresh_rate" format="integer" />
        <attr name="heart_degrees_interval_min" format="integer" />
        <attr name="heart_degrees_interval_max" format="integer" />
    </declare-styleable>
</resources>